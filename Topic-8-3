#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    char *locale = setlocale(LC_ALL, "");
    int a = 0;
    int b = 0;
    int answer = 0;
    printf("Введите A: ");
    scanf("%d", &a);
    printf("Введите B: ");
    scanf("%d", &b);
    answer = a%b;
    printf("Незанятая часть: %d", answer);
    return 0;
}

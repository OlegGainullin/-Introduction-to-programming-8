#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    char *locale = setlocale(LC_ALL, "");
    int a = 0;
    int answer = 0;
    printf("Введите двузначное число: ");
    scanf("%d", &a);
    answer = 10*(a%10) + (a/10);
    printf("Перестановка цифр: %d", answer);
    return 0;
}
